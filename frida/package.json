{
    "name": "playground",
    "main": "index.ts",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "frida-compile -o ./dist/_.js ./src/index.ts",
        "watch": "frida-compile -w -o ./dist/_.js ./src/index.ts",
        "attach": "run() { frida -n \"$1\" -l ./dist/_.js --runtime=v8; }; run",
        "spawn": "run() { frida -f \"$1\" -l ./dist/_.js --runtime=v8; }; run",
        "l4-spawn": "npm run spawn com.oddno.lovelive",
        "l4": "npm run attach \"リンクラ\""
    },
    "devDependencies": {
        "@types/frida-gum": "^19.0.0",
        "@types/node": "^18.19.121",
        "frida-il2cpp-bridge": "^0.12.1"
    },
    "dependencies": {
        "frida-compile": "^19.0.4",
        "frida-objc-bridge": "^8.0.5"
    }
}
